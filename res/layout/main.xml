<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/LinearLayout01"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">
	
		
	<TableLayout android:id="@+id/TableLayout01"
		android:layout_width="wrap_content" android:layout_height="fill_parent">
	<ImageView android:id="@+id/ImageView01"
							android:layout_width="310dp" android:layout_height="150dp"
				android:layout_margin="5dp"
				android:src="@drawable/sun_icon"
				></ImageView>
		

<TableRow android:id="@+id/TableRow02" android:layout_width="fill_parent"
			android:layout_height="wrap_content">

			<ImageButton android:id="@+id/time_button"
				android:layout_width="95dp" android:layout_height="90dp"
				android:minWidth="64dp" android:minHeight="64dp" android:scaleType="fitXY"
				android:layout_margin="5dp" android:src="@drawable/sun_icon"></ImageButton>


			<ImageButton android:id="@+id/jupiter_button"
				android:layout_width="95dp" android:layout_height="90dp"
				android:minWidth="64dp" android:minHeight="64dp" android:scaleType="fitXY"
				android:layout_margin="5dp" android:src="@drawable/jupiter_icon">

			</ImageButton>
			
			
			<ImageButton android:id="@+id/saturn_button"
			
				android:layout_width="95dp" android:layout_height="90dp"
				android:minWidth="64dp" android:minHeight="64dp" android:scaleType="fitXY"
				android:layout_margin="5dp" android:src="@drawable/saturn_icon"></ImageButton>
			
		</TableRow>
<TableRow android:id="@+id/TableRow03" android:layout_width="fill_parent"
			android:layout_height="100dp">

			<ImageButton android:id="@+id/saturn_button"
				android:layout_width="90dp" android:layout_height="90dp"
				android:minWidth="64dp" android:minHeight="64dp" android:scaleType="fitXY"
				android:layout_margin="5dp" android:src="@drawable/saturn_icon"></ImageButton>


			<ImageButton android:id="@+id/support_button"
				android:layout_width="90dp" android:layout_height="90dp"
				android:minWidth="64dp" android:minHeight="64dp" android:scaleType="fitXY"
				android:layout_margin="5dp" android:src="@drawable/moon_icon">

			</ImageButton>
		</TableRow>
		
	</TableLayout>


</LinearLayout>
